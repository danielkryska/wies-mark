  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "ionic build --prod -- --base-href https://danielkryska.github.io/wies-mark/ && npx angular-cli-ghpages --dir=www",
